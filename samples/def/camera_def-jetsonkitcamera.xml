		<description>Video Resolution</description>
		<options>
			<option name="3264 x 2464 21fps (UHD)" value="0">
				<exclusions>
					<exclude>CAM_VIDFMT</exclude>
				</exclusions>
			</option>
			<option name="3264 x 1848 28fps (UHD)" value="1">
				<exclusions>
					<exclude>CAM_VIDFMT</exclude>
				</exclusions>
			</option>
			<option name="1920 x 1080 30fps (UHD)" value="2">
