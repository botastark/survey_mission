		<vendor>vi-output, imx219 10-0010</vendor>
	</definition>
	<parameters>
		<parameter name="CAM_MODE" type="uint32" default="1" control="0">
			<description>Camera Mode</description>
			<options>
				<option name="Photo" value="0" />
